	#problemset-edit
		.block
			p.header Edit Problem Set
			form#jfps-ed.judge-form(auto-complete="off")
				input#jfps-ed-i(name="i", type="hidden")
				.row
					input#jfps-ed-n.judge-form-string(name="n", type="text", placeholder="Name", autocomplete="off")
