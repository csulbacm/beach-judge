				input#jfp-ed-s(name="s", type="hidden")
				input#jfp-ed-i(name="i", type="hidden")
				.row
					input#jfp-ed-n.judge-form-string(name="n", type="text", placeholder="Name", autocomplete="off")
				.row
					input#jfp-ed-cl.form-button(value="Clear", type="button")
					input#jfp-ed-up.form-button(value="Update", type="button")
					input#jfp-ed-dl.form-button(value="Delete", type="button")
					input#jfp-ed-ca.form-button(value="Cancel", type="button")
				.row.hidden
					p#jfp-ed-er.form-error
		.block
			p.header Problem Description
			form#jfp-upd.judge-form(auto-complete="off")
				.row
					input#jfp-upd-f(name="f", type="file")
				.row
					input#jfp-upd-vw.form-button(value="View", type="button")
					input#jfp-upd-up.form-button(value="Upload", type="button")
		.block
			p.header Sample Test Files
			form#jfp-upt.judge-form(auto-complete="off")
				.row
