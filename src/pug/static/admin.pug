					input#jfps-cr-cr.form-button(value="Create", type="button")
					input#jfps-cr-ca.form-button(value="Cancel", type="button")
				.row.hidden
					p#jfps-cr-er.form-error
				input#jfps-cr-o(name="o", type="hidden")
